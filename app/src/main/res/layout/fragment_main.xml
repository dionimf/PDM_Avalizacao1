    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.dionimfxgmail.avaliacaopdm.MainActivityFragment"
    tools:showIn="@layout/activity_main"
    android:layout_margin="@dimen/margem_botoes_calduladora"
    android:layout_marginBottom="@dimen/margem_botoes_calduladora"
    android:layout_marginEnd="@dimen/margem_botoes_calduladora"
    android:layout_marginLeft="@dimen/margem_botoes_calduladora"
    android:layout_marginRight="@dimen/margem_botoes_calduladora"
    android:layout_marginStart="@dimen/margem_botoes_calduladora"
    android:layout_marginTop="@dimen/margem_botoes_calduladora"
    android:background="@color/colorPrimaryDark">


    <TextView
        android:text="@string/tv_fragmento"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/tv_fragmento"
        android:textSize="36sp" />
</RelativeLayout>
